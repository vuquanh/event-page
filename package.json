  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vuquanh/event-page/issues"
  },
  "homepage": "https://github.com/vuquanh/event-page#readme",
  "dependencies": {
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^5.2.0",
    "mongoose": "^7.0.4",
    "nodemon": "^2.0.22"
  }
}
